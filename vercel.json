{
  "rewrites": [

   
    {
      "source": "/assets/(.*)",
      "destination": "https://morfys.vercel.app/assets/$1"
    },
    {
      "source": "/@vite/(.*)",
      "destination": "https://morfys.vercel.app/@vite/$1"
    },
    {
      "source": "/(.*)\\.(js|css)",
      "destination": "https://morfys.vercel.app/$1.$2"
    },
    {
      "source": "/reset-password/(.*)",
      "destination": "https://morfys.vercel.app/reset-password/$1"
    },
    {
      "source": "/panel",
      "destination": "https://morfys-local.vercel.app/"
    },
    {
      "source": "/panel/(.*)",
      "destination": "https://morfys-local.vercel.app/$1"
    },
    {
      "source": "/:local",
      "destination": "https://morfys.vercel.app/:local"
    },
    {
      "source": "/:local/(.*)",
      "destination": "https://morfys.vercel.app/:local/$1"
    }
  ]
}